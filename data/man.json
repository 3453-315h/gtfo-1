{
  "description": "This invokes the default pager, which is likely to be  'less', other functions may apply.",
  "functions": {
    "shell": [
      {
        "code": "man man\n!/bin/sh\n"
      }
    ],
    "file-read": [
      {
        "code": "man [file]"
      }
    ],
    "sudo": [
      {
        "code": "sudo man man\n!/bin/sh\n"
      }
    ]
  }
}